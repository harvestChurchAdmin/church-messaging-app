# Breeze API Credentials
BREEZE_API_KEY=your_breeze_api_key_here
BREEZE_SUBDOMAIN=your_breeze_subdomain_here

# Twilio Credentials
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here # e.g., +1234567890

# Server Configuration
SERVER_BASE_URL=http://localhost:3000 # Important for Twilio callbacks (use your public URL in .env)
SESSION_SECRET=a_long_random_string_for_session_secret

# Google OAuth Credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback # Update if your domain changes

# Application Logo URL
APP_LOGO_URL=https://placehold.co/280x80/cccccc/333333?text=Your+Logo+Here # URL for your application logo
