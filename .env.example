# .env.example

# Your Breeze API Key
# You can find this in your Breeze account settings, usually under Integrations > API
BREEZE_API_KEY=your_breeze_api_key_here

# Your Breeze subdomain (e.g., if your Breeze URL is harvestnanaimo.breezechms.com, your subdomain is harvestnanaimo)
BREEZE_SUBDOMAIN=harvestnanaimo

# Twilio Account SID
# Find this in your Twilio Console: https://www.twilio.com/console
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here

# Twilio Auth Token
# Find this in your Twilio Console: https://www.twilio.com/console
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# Your Twilio Phone Number (e.g., +1234567890)
# This is the phone number you purchased from Twilio that will send messages
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Port for your web server (optional, defaults to 3000 in server.js)
# PORT=3000